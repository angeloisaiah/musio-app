{
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^24.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "name": "musio",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "prepare": "husky install",
    "qcheck": "bash ./scripts/qcheck",
    "qnew": "bash ./scripts/qnew",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "qplan": "bash ./scripts/qplan",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "qcode": "bash ./scripts/qcode",
    "prettier:check": "prettier --check .",
    "dev": "turbo run dev --parallel",
    "dev:api": "pnpm --filter @musio/api dev",
    "migrate": "pnpm --filter @musio/api exec tsx src/migrate.ts up",
    "migrate:down": "pnpm --filter @musio/api exec tsx src/migrate.ts down",
    "seed": "pnpm --filter @musio/api exec tsx src/seed.ts",
    "dev:web": "pnpm --filter @musio/web dev",
    "dev:worker": "pnpm --filter @musio/api exec tsx src/worker.ts"
  }
}